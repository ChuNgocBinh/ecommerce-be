{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": 1,
        "quotes": [
            2,
            "single"
        ],
        "no-tabs": 0,
        "no-unused-vars": 1,
        "max-len": [
            0,
            {
                "code": 120,
                "tabWidth": 4
            }
        ],
        "linebreak-style": [
            0,
            "unix"
        ],
        "no-console": [
            "warn",
            {
                "allow": [
                    "warn",
                    "error"
                ]
            }
        ],
        "no-underscore-dangle": "off"
    }
}